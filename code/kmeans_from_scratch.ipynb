   "source": [
    "def distance_threeD(a, b):\n",
    "    distance_threeD = calculate_distance(a[0], b[0]) + calculate_distance(a[1], b[1]) + calculate_distance(a[2], b[2])\n",
    "    return distance_threeD\n",
    "a = [1,0,0]\n",
    "B = [0,1,0]\n",
    "print(distance_threeD(a, b))"
   ]
  },
  {
