    "def assignment(image, centroids):\n",
    "    old_centers = set()\n",
    "    new_centers = initiate_centroids(centroids)\n",
    "   \n",
    "    while old_centers != new_centers:\n",
    "        old_centers = new_centers\n",
    "        clusters = dict([(key, []) for key in new_centers])\n",
    "\n",
    "    for p in ndindex(image.shape):\n",
    "        distances_pc = {}\n",
    "        for c in new_centers:\n",
    "            distance_pc = calculate_3D_distance(p, c)\n",
    "            distances_pc[c] = distance_pc\n",
    "        bestCenter = min(distances_pc.items(), key=operator.itemgetter(1))[0]\n",
    "        clusters[bestCenter].append(p)\n",
    "    return clusters\n"
   ]
